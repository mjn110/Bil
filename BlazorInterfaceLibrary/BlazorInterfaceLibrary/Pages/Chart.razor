<div class="col-12 col-md">
    <div class="card border shadow mb-4">
        <div class="card-body ms-5" style="height: 200px;">
            <div class="row">
                <div class="col col-lg-5 d-flex justify-content-center">
                    <div class="pie m-0" style="--p:100; --c: lightgray">Sell</div>
                    <div class="pie m-0" style="--p:90; --b:20px; --c: #007bff">&nbsp;</div>
                    <div class="pie m-0" style="--p:50; --b:20px; --c: #dc3545">&nbsp;</div>
                    <div class="pie m-0" style="--p:30; --b:20px; --c: #ebba34">&nbsp;</div>
                </div>
                <div class="col col-lg-7 d-none d-lg-block">
                    <div class="row d-flex flex-column align-items-center h-100">
                        <div class="col m-1">
                            <span class="badge rounded-pill bg-warning d-inline mx-1">&nbsp;&nbsp;&nbsp;&nbsp;</span><small class="d-inline">Personal</small>
                        </div>
                        <div class="col m-1">
                            <span class="badge rounded-pill bg-danger d-inline mx-1">&nbsp;&nbsp;&nbsp;&nbsp;</span><small class="d-inline">Self</small>
                        </div>
                        <div class="col m-1">
                            <span class="badge rounded-pill bg-primary d-inline mx-1">&nbsp;&nbsp;&nbsp;&nbsp;</span><small class="d-inline">Official</small>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-footer border-0">
            <p class="h6 m-0 text-center">
                Last month sell
            </p>
        </div>
    </div>
</div>

